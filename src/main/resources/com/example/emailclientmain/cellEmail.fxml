<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListCell?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root type="javafx.scene.control.ListCell" xmlns="http://javafx.com/javafx/18.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <graphic>
      <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="30.0">
   <children>
      <HBox alignment="CENTER_LEFT" prefWidth="180.0" spacing="20.0">
         <children>
            <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@./images/user.png" />
               </image>
            </ImageView>
            <Label fx:id="account" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefWidth="300.0" spacing="20.0">
         <children>
            <Label fx:id="text"/>
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" prefWidth="60.0" spacing="20.0">
         <children>
            <Label fx:id="time" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="60.0" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
   </graphic>
</fx:root>
