<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.control.ListCell?>

<fx:root type="javafx.scene.control.ListCell" xmlns="http://javafx.com/javafx/8.0.141"  xmlns:fx="http://javafx.com/fxml/1">
   <graphic>
      <HBox alignment="CENTER"  maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="30.0" >
   <children>
      <HBox alignment="CENTER" spacing="20.0" >
         <children>
            <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@./images/user.png" />
               </image>
            </ImageView>
            <Label fx:id="account" />
         </children>
      </HBox>
      <Label fx:id="text"/>
      <Label fx:id="time" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
   </graphic>
</fx:root>
